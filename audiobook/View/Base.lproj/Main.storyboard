                                            </imageView>
                                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFit" horizontalHuggingPriority="251" verticalHuggingPriority="251" fixedFrame="YES" image="мертвые души" translatesAutoresizingMaskIntoConstraints="NO" id="aPC-8e-Gvl">
